{
      "name": "flowphone",
      "version": "1.0.0",
      "description": "Sistema de gestión de líneas telefónicas",
      "scripts": {
            "start": "react-scripts start",
            "build": "react-scripts build",
            "test": "react-scripts test",
            "eject": "react-scripts eject",
            "backend": "cd backend && npm start",
            "dev": "concurrently \"npm run backend\" \"npm start\"",
            "install-all": "npm install && cd backend && npm install",
            "setup": "npm run install-all && npm run dev",
            "setup-db": "cd backend && npm run setup-db",
            "test-db": "cd backend && npm run test-db"
      },
      "dependencies": {
            "autoprefixer": "^10.0.0",
            "framer-motion": "^12.23.12",
            "lucide-react": "^0.534.0",
            "node-fetch": "^2.7.0",
            "postcss": "^8",
            "react": "^18.0.0",
            "react-dom": "^18.0.0",
            "react-router-dom": "^6.30.1",
            "react-scripts": "^5.0.1",
            "tailwind-merge": "^2.4.0",
            "tailwindcss": "^3.4.1",
            "tailwindcss-animate": "^1.0.7"
      },
      "devDependencies": {
            "concurrently": "^8.2.2"
      },
      "main": "/index.js",
      "browserslist": {
            "production": [
                  ">0.2%",
                  "not dead",
                  "not op_mini all"
            ],
            "development": [
                  "last 1 chrome version",
                  "last 1 firefox version",
                  "last 1 safari version"
            ]
      }
}
